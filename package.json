{
  "name": "jsx-inject",
  "version": "0.0.2",
  "private": false,
  "description": "A CLI tool to batch-modify React components that match specific conditions.",
  "keywords": [
    "react",
    "jsx",
    "inject",
    "cli"
  ],
  "repository": "https://github.com/hyi86/jsx-inject.git",
  "license": "MIT",
  "author": "henry hong <hyi8656@gmail.com>",
  "type": "module",
  "bin": {
    "jsx-inject": "./bin/index.js"
  },
  "scripts": {
    "build": "tsup",
    "postbuild": "chmod +x bin/*",
    "check-types": "tsc --noEmit",
    "dev": "tsx src/index.ts",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "fast-glob": "^3.3.3",
    "prettier": "^3.5.3",
    "ts-morph": "^26.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-only-warn": "^1.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "packageManager": "pnpm@10.11.1",
  "publishConfig": {
    "access": "public"
  }
}
